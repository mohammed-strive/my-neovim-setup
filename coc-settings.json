{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "json",
    "javascriptreact",
    "typescript.tsx"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "diagnostic.errorSign": "•",
  "diagnostic.warningSign": "•",
  "diagnostic.infoSign": "•",
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": [
        "go.mod"
      ],
      "filetypes": [
        "go"
      ],
      "disableWorkspaceFolders": true
    },
    "intelephense": {
      "command": "intelephense",
      "args": [
        "--stdio"
      ],
      "filetypes": [
        "php"
      ],
      "initializationOptions": {
        "storagePath": "/tmp/intelephense"
      }
    },
    "graphql": {
      "command": "graphql-lsp",
      "args": [
        "server",
        "-m",
        "stream"
      ],
      "filetypes": [
        "typescript",
        "typescriptreact",
        "graphql"
      ]
    },
    "nim": {
      "command": "nimlsp",
      "filetypes": [
        "nim"
      ]
    },
    "sql": {
      "command": "sql-language-server",
      "args": [
        "up",
        "--method",
        "stdio"
      ],
      "filetypes": [
        "sql",
        "mysql"
      ]
    },
    "clojure-lsp": {
      "command": "bash",
      "args": [
        "-c",
        "clojure-lsp"
      ],
      "rootPatterns": [
        "project.clj"
      ],
      "filetypes": [
        "clojure"
      ],
      "additionalSchemes": [
        "jar",
        "zipFile"
      ],
      "trace.server": "verbose",
      "initializationOptions": {}
    }
  }
}
